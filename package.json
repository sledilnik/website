    "highcharts": "^8.1.2",
    "highcharts-react-official": "^3.0.0",
    "i18next": "^19.6.2",
    "i18next-browser-languagedetector": "^5.0.0",
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",

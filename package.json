{
    "name": "covid19-web",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "dotnet fable watch src/visualizations/ --runFast vite",
        "build": "dotnet fable watch src/visualizations/ --run vite build",
        "test:unit": "vue-cli-service test:unit",
        "lint": "vue-cli-service lint",
        "test": "vue-cli-service test:unit",
        "dotnet-restore": "dotnet restore --configfile src/visualizations/NuGet.Config SloCovid19Website.sln",
        "test-fsharp": "dotnet test"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.4.0",
        "@fortawesome/free-solid-svg-icons": "^6.4.0",
        "@fortawesome/vue-fontawesome": "^2.0.10",
        "@panter/vue-i18next": "^0.15.2",
        "@vue-stripe/vue-stripe": "^4.5.0",
        "axios": "^1.6.2",
        "bootstrap-vue": "^2.23.1",
        "d3": "^7.8.4",
        "date-fns": "^2.29.3",
        "highcharts": "^11.2.0",
        "highcharts-react-official": "^3.2.1",
        "i18next": "^22.4.14",
        "i18next-browser-languagedetector": "^7.0.1",
        "marked": "^11.0.1",
        "rc-slider": "^9.5.4",
        "react": "^16.14.0",
        "react-autosuggest": "^10.1.0",
        "react-dom": "^16.14.0",
        "robotstxt-webpack-plugin": "^7.0.0",
        "typescript": "^5.0.3",
        "v-scroll-lock": "^1.3.1",
        "vue": "^2.7",
        "vue-clickaway": "^2.2.2",
        "vue-date-fns": "^2.0.2",
        "vue-fuse": "^3.0.2",
        "vue-meta": "^2.4.0",
        "vue-router": "^3.6.5",
        "vue-scrollto": "^2.18.2",
        "vuex": "^3.5.1"
    },
    "devDependencies": {
        "@vitejs/plugin-vue2": "^2.3.1",
        "babel-eslint": "^10.1.0",
        "chai": "^4.3.7",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^6.2.2",
        "fable-compiler": "^2.13.0",
        "prettier": "3.1.1",
        "remotedev": "^0.2.7",
        "sass": "^1.26.11",
        "vite": "^5.0.4",
        "vue-template-compiler": "^2.7.14"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "overrides": [
            {
                "files": [
                    "**/__tests__/*.{j,t}s?(x)",
                    "**/tests/unit/**/*.spec.{j,t}s?(x)"
                ],
                "env": {
                    "jest": true
                }
            }
        ]
    },
    "postcss_no": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
